{
  "name": "taysway.xyz",
  "version": "0.0.1",
  "description": "Let's make an alphabet of Taylor Swift things!",
  "main": "taysway-spa.jsx",
  "repository": "git@github.com:suitupalex/taysway.xyz",
  "author": "Alexander Martin <alex@suitupalex.com>",
  "license": "MIT",
  "private": true,
  "pre-commit": [
    "precommit-message",
    "test"
  ],
  "scripts": {
    "build": "./scripts/build",
    "lint": "./scripts/lint",
    "posttest": "./scripts/posttest",
    "precommit-message": "./scripts/precommit-message",
    "prod": "./scripts/prod",
    "test": "./scripts/test",
    "watch": "./scripts/watch"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.13",
    "codacy-coverage": "^2.0.3",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.28.7",
    "ctrl-env": "^2.2.0",
    "ctrl-react-component": "^1.1.0",
    "ctrl-react-router": "^2.3.0",
    "debug": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-ctrl": "^4.1.0",
    "eslint-config-ctrl-react": "^5.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.7",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.5.0",
    "pug-lint-config-ctrl": "^1.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0",
    "react-test-renderer": "^16.0.0",
    "sass-lint": "^1.12.0",
    "sass-lint-config-ctrl": "^2.0.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "semantic-ui-react": "^0.74.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0"
  },
  "jest": {
    "testRegex": "tests/.*.jsx$",
    "testURL": "https://taysway.xyz",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/tests/lib/stubs/object.js",
      "\\.svg$": "<rootDir>/tests/lib/stubs/string.js"
    },
    "transformIgnorePatterns": [],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "verbose": true
  },
  "nyc": {
    "extension": [
      ".jsx"
    ]
  },
  "dependencies": {
    "lodash.transform": "^4.6.0"
  }
}
